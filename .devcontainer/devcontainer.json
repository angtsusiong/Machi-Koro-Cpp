{
    "name": "machi-koro-cpp-devcontainer",
    "build": {
        "dockerfile": "./Dockerfile",
        "context": "."
    },
    "features": {
        "ghcr.io/devcontainers/features/common-utils:2": {
            "installZsh": "false",
            "installOhMyZsh": "false",
            "installOhMyZshConfig": "false",
            "username": "vscode",
            "userUid": "1000",
            "userGid": "1000",
            "upgradePackages": "true"
        }
    },
    "customizations": {
        "vscode": {
            "settings": {},
            "extensions": []
        }
    },
    "remoteUser": "vscode"
}
